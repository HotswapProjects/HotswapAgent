<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.hotswap.agent.plugin.mybatis.springboot.BootUserMapper">

    <select id="getUserXML" resultType="org.hotswap.agent.plugin.mybatis.springboot.BootUser">
        select email from `boot_user` where name = #{name}
    </select>

</mapper>
